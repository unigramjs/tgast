{
	"name": "tgast",
	"version": "3.0.0-alpha.8",
	"description": "Telegram Abstract Syntax Tree format.",
	"type": "module",
	"license": "MIT",
	"homepage": "https://github.com/unigramjs/tgast#readme",
	"bugs": {
		"url": "https://github.com/unigramjs/tgast/issues"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/unigramjs/tgast.git"
	},
	"author": {
		"name": "Quadratz",
		"email": "quadratz@proton.me",
		"url": "https://t.me/quadratz"
	},
	"files": [
		"dist",
		"LICENSE",
		"package.json",
		"README.md"
	],
	"keywords": [
		"telegram",
		"bot",
		"api",
		"entity",
		"entities",
		"ast",
		"syntax",
		"tree",
		"unified",
		"unifiedjs",
		"unified.js",
		"unist",
		"unigramjs",
		"unigram.js"
	],
	"main": "./dist/index.cjs",
	"module": "./dist/index.js",
	"types": "./dist/index.d.cts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs"
		},
		"./package.json": "./package.json"
	},
	"publishConfig": {
		"access": "public"
	},
	"devEngines": {
		"runtime": [
			{
				"name": "node",
				"version": ">=20.19",
				"onFail": "warn"
			}
		]
	},
	"scripts": {
		"dev": "tsdown --watch",
		"build": "tsdown",
		"check": "tsc --noEmit && biome check",
		"check:fix": "biome check --write",
		"test": "vitest run",
		"test:watch": "vitest watch",
		"debug:test": "DEBUG='true' vitest run",
		"debug:test:watch": "DEBUG='true' vitest watch",
		"release": "changelogen --release",
		"release:github": "changelogen gh release"
	},
	"devDependencies": {
		"@biomejs/biome": "2.2.2",
		"changelogen": "^0.6.2",
		"tsdown": "^0.14.2",
		"typescript": "^5.9.2"
	},
	"dependencies": {
		"@types/unist": "^3.0.3"
	}
}
